{
  "name": "bizday",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "ava",
    "compile": "coffee -o ./ -cw coffee/",
    "watch": "npm run compile &ava -w",
    "cover": "nyc ava",
    "report": "nyc report --reporter=html",
    "update:snap": "ava --update-snapshots",
    "buildCalendar": "coffee build_calendar.coffee"
  },
  "keywords": [],
  "dependencies": {
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "rrq": "^1.1.1"
  },
  "devDependencies": {
    "bluebird": "^3.5.1",
    "coffeescript": "^2.2.3",
    "yaml": "^1.0.0-rc.7"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.js"
    ],
    "failFast": false,
    "serial": false,
    "tap": false,
    "verbose": true,
    "color": true,
    "watch": false
  },
  "main": "lib/index.js",
